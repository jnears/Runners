<h1>Editing post</h1>

<%= render 'form' %>

<button type="button" class="btn btn-default"><%= link_to 'Show', @post %></button>
<button type="button" class="btn btn-default"><%= link_to 'Back', posts_path %></button>
<button type="button" class="btn btn-danger"><%= link_to 'Destroy', @post, method: :delete, data: { confirm: 'Are you sure?' } %></button>
